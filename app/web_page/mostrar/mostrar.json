    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.solicitudAperturaCuenta ){",
        "\treturn {",
        "\t\t//map solicitudAperturaCuenta variable to expected task contract input",
        "\t\tsolicitudAperturaCuentaInput: {",
        "\t\t\tnombre: $data.solicitudAperturaCuenta.nombre !== undefined ? $data.solicitudAperturaCuenta.nombre : null,",
        "\t\t\tdni: $data.solicitudAperturaCuenta.dni !== undefined ? $data.solicitudAperturaCuenta.dni : null,",
        "\t\t\tcorreo: $data.solicitudAperturaCuenta.correo !== undefined ? $data.solicitudAperturaCuenta.correo : null,",
        "\t\t\tvalidarDatos: $data.solicitudAperturaCuenta.validarDatos !== undefined ? $data.solicitudAperturaCuenta.validarDatos : null,",
        "\t\t\tpersonaJuridica: $data.solicitudAperturaCuenta.personaJuridica !== undefined ? $data.solicitudAperturaCuenta.personaJuridica : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
